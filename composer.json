{
  "name": "tourze/wechat-mini-program-plugin-bundle",
  "description": "微信小程序插件",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "monolog/monolog": "^3.1",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/framework-bundle": "^7.4",
    "symfony/http-foundation": "^7.4",
    "symfony/http-kernel": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/json-rpc-core": "2.0.*",
    "tourze/symfony-dependency-service-loader": "1.0.*",
    "tourze/wechat-mini-program-bundle": "2.0.*",
    "yiisoft/json": "^1.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-kernel-test": "2.0.*",
    "tourze/phpunit-symfony-unit-test": "2.0.*"
